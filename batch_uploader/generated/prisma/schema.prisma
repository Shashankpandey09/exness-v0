generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

// model User{
//   id Int @id @default(autoincrement())
//   balance String 
// }
model Trade {
  time        DateTime @db.Timestamptz
  symbol      String
  price       Decimal  @db.Decimal(18, 8)
  size        Decimal  @db.Decimal(18, 8)
  side        String?
  exchange_id String?

  @@id([symbol, time])
  @@map("trades")
}
